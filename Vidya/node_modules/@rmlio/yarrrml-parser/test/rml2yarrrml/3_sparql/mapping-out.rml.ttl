@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix : <http://mapping.example.com/> .
@prefix crml: <http://semweb.mmlab.be/ns/crml#> .
@prefix mail: <http://example.com/mail#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix ex: <http://example.com/> .
@prefix hydra: <http://www.w3.org/ns/hydra/core#> .

<http://example.com/base#InputSPARQL> rdf:type sd:Service ;
        sd:endpoint <http://localhost:PORT/ds1/sparql> ;
        sd:supportedLanguage sd:SPARQL11Query ;
        sd:resultFormat <http://www.w3.org/ns/formats/SPARQL_Results_JSON> .

ex:TriplesMap1 rdf:type rr:TriplesMap ;
        rml:logicalSource _:n3-8 ;
        rr:subjectMap _:n3-9 ;
        rr:predicateObjectMap _:n3-10 .

_:n3-8 rml:source <http://example.com/base#InputSPARQL> ;
        rml:referenceFormulation ql:JSONPath ;
        rml:iterator "$.results.bindings[*]" ;
        rml:query "\r\n            PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>\r\n            PREFIX foaf: <http://xmlns.com/foaf/0.1/>\r\n            PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>\r\n            SELECT ?Student\r\n            WHERE {\r\n                ?x  a <http://example.com/Student> ;\r\n
 foaf:name  ?Student .\r\n            } " .

_:n3-9 rr:template "http://example.com/{Student.value}" ;
        rr:class ex:Student .

_:n3-10 rr:predicate foaf:name ;
        rr:objectMap _:n3-11 .

_:n3-11 rml:reference "Student.value" .

ex:TriplesMap2 rdf:type rr:TriplesMap ;
        rml:logicalSource _:n3-12 ;
        rr:subjectMap _:n3-13 ;
        rr:predicateObjectMap _:n3-14 .

_:n3-12 rml:source <http://example.com/base#InputSPARQL> ;
        rml:referenceFormulation ql:JSONPath ;
        rml:iterator "$.results.bindings[*]" ;
        rml:query "\r\n                PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>\r\n                PREFIX foaf: <http://xmlns.com/foaf/0.1/>\r\n                PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>\r\n                SELECT ?Sport\r\n                WHERE {\r\n                    ?x  a <http://example.com/Student> ;\r\n                        <http://example.com/plays> ?sportObject .\r\n                    ?sportObject foaf:name ?Sport .\r\n                } " .

_:n3-13 rr:template "http://example.com/{Sport.value}" ;
        rr:class ex:Sport .

_:n3-14 rr:predicate foaf:name ;
        rr:objectMap _:n3-15 .

_:n3-15 rml:reference "Sport.value" .